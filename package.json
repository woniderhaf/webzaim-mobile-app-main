  "scripts": {
    "android": "ENVFILE=.env.production react-native run-android",
    "android-dev": "react-native run-android --variant=debug",
    "ios": "react-native run-ios",
    "ios-se": "react-native run-ios --simulator='iPhone SE (3rd generation)'",
    "ios-dev": "react-native run-ios --scheme \"DEV\"",
    "ios-dev-se": "react-native run-ios --scheme \"DEV\" --simulator='iPhone SE (3rd generation)'",
    "start": "react-native start",
    "clean": "./android/gradlew clean",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "assembleReleaseDebug": "cd android && ./gradlew clean && ENVFILE=.env.development ./gradlew assembleRelease",
    "assembleRelease": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "bundleRelease": "cd android && ./gradlew clean && ./gradlew bundleRelease"
  },
